                    {
                        "id": 120800,
                        "voiceFull": "fullvoice/section_102004/vo_full_102004-6-10",
                        "voiceFullStop": "fullvoice/section_102004/vo_full_102004-6-9"
                    }
                ],
                "textLeft": "¡Cuando todo ese poder sea liberado,@estallará creando magníficas explosions!"
