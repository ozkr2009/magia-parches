autoTurnLast":1.0,"bg":"bg_adv_19081.jpg","chara":[{"cheek":0,"face":"mtn_ex_001.exp.json","id":100601,"motion":0,"pos":0},{"cheek":0,"face":"mtn_ex_040.exp.json","id":200501,"motion":100,"pos":2}],"surround":"stop","turnChangeIn":"fadeIn","turnEffect":"blackFrame"},{"chara":[{"id":100601,"motion":100}],"nameLeft":"Mifuyu","nameRight":"Mami","textLeft":"Por cierto, ¿qué pasó con las otras Chicas@Mágicas de Mitakihara?","turnChangeOut":"fadeOut"},{"chara":[{"id":100601}],"textLeft":"No las dejaste así sin más, ¿verdad?"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":200501,"motion":200}],"textRight":"Por supuesto que no."},{"chara":[{"id":200501}],"textRight":"Tuve que encerrarlas para que no pudieran@salir de la Ciudad de Mitakihara."},{"autoTurnLast":1.0,"bg":"bg_adv_19042.jpg","chara":[{"cheek":0,"face":"mtn_ex_020.exp.json","id":200601,"motion":0,"pos":1}],"turnChangeIn":"fadeIn","turnEffect":"stop"},{"chara":[{"id":200601,"lipSynch":0,"motion":300}],"nameCenter":"Kyoko","surround":"7003_wind","textCenter":"...","turnChangeOut":"fadeOut"},{"chara":[{"id":200601}],"textCenter":"Supongo que podría pasar por Mitakihara."},{"chara":[{"eyeClose":1,"id":200601}],"textCenter":"Después de todo, les dije que compartiríamos@información."},{"autoTurnLast":2.0,"turnChangeIn":"fadeIn"}]},"version":3}