        "chara": [
          {
            "cheek": 0,
            "face": "mtn_ex_021.exp.json",
            "id": 100101,
            "motion": 0,
            "voiceFull": "fullvoice/section_101601/vo_full_101601-6-54",
            "voiceFullStop": "fullvoice/section_101601/vo_full_101601-6-53"
          }
        ],
        "textLeft": "Tampoco quiero pelearme contigo sin@saber toda la historia."
