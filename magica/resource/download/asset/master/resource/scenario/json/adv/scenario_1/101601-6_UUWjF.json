        "chara": [
          {
            "cheek": 0,
            "face": "mtn_ex_010.exp.json",
            "id": 100601,
            "voiceFull": "fullvoice/section_101601/vo_full_101601-6-57",
            "voiceFullStop": "fullvoice/section_101601/vo_full_101601-6-56"
          }
        ],
        "textRight": "Me alegro mucho de que hayas dicho eso."
