ullStop":"fullvoice/section_101855/vo_full_101801-10-17"},{"autoTurnLast":0.01,"nameNarration":"Walpurgisnacht","narrationBg":"black","narrationEffect":"in","turnChangeOut":"fadeOut"},{"narration":"¡Ah ja ja! ¡Ahh ja ja ja!","voiceFull":"fullvoice/section_101855/vo_full_101801-10-18"},{"autoTurnLast":0.01,"bg":"bg_adv_19121.jpg","chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100101,"motion":0,"pos":0,"soulGem":0},{"cheek":0,"face":"mtn_ex_010.exp.json","id":100701,"motion":0,"pos":2}],"narrationEffect":"out","turnChangeIn":"fadeIn","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-18"},{"chara":[{"id":100101,"motion":200,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-19"}],"textLeft":"¿Risas? ¿Quién es esa, Touka?","turnChangeOut":"fadeOut"},{"chara":[{"id":100701,"motion":1,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-20","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-19"}],"textRight":"Pues es una bruja, por supuesto."},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":100101,"motion":401,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-21","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-20"}],"textLeft":"¡¿Una bruja?!"},{"chara":[{"id":100701,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-22","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-21"}],"textRight":"Sí. Es una leyenda entre nosotras,@las Chicas Mágicas."},{"chara":[{"id":100701,"motion":201,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-23","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-22"}],"textRight":"¡La mayor calamidad de nuestro tiempo!"},{"chara":[{"id":100701,"motion":100,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-24","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-23"}],"textRight":"Hoy, convocamos a Walpurgisnacht."},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":100701,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-25","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-24"}],"textRight":"*Risitas*"},{"chara":[{"cheek":0,"face":"mtn_ex_030.exp.json","id":100101,"motion":100,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-26","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-25"}],"textLeft":"¿Walpurgisnacht?"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100701,"motion":0,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-27","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-26"}],"textRight":"Oh, ya veo. No la conoces, ¿Verdad,@Iroha Tamaki?"},{"chara":[{"id":100701,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-28","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-27"}],"textRight":"Walpurgisnacht, como es conocida por@las Chicas Mágicas..."},{"chara":[{"id":100701,"motion":400,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-29","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-28"}],"textRight":"...¡Es una bruja increíblemente poderosa!"},{"chara":[{"id":100101,"motion":300,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-30","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-29"}],"textLeft":"¿Invocaste a una bruja? ¿Es eso posible?"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":100701,"motion":200,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-31","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-30"}],"textRight":"¿No te has dado cuenta de que todas las Brujas@y Chicas Mágicas se reúnen en esta ciudad?"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":100701,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-32","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-31"}],"textRight":"*Risitas*"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100701,"motion":100,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-33","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-32"}],"textRight":"Hemos trabajado mucho robando ondas de radio@y electricidad."},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":100101,"motion":401,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-34","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-33"}],"textLeft":"¡¿Significa eso que están detrás de todo lo@que ha pasado hoy?!"},{"chara":[{"cheek":0,"face":"mtn_ex_011.exp.json","id":100701,"motion":400,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-35","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-34"}],"textRight":"¡Claro que sí! ¡Ganaste 100 puntos!"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100701,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-36","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-35"}],"textRight":"Todo era necesario para convocar a Walpurgisnacht."},{"chara":[{"cheek":1,"face":"mtn_ex_021.exp.json","id":100101,"motion":400,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-37","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-36"}],"textLeft":"¡Pasarán cosas horribles aquí si invocas a una@bruja tan peligrosa!"},{"chara":[{"cheek":0,"face":"mtn_ex_050.exp.json","id":100701,"motion":1,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-38","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-37"}],"textRight":"Pero eso no tiene nada que ver contigo,@¿Verdad, Iroha Tamaki?"},{"chara":[{"cheek":0,"face":"mtn_ex_010.exp.json","id":100701,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-39","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-38"}],"textRight":"Me refiero a que pronto serás comida de bruja."},{"chara":[{"id":100701,"motion":100,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-40","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-39"},{"id":100101,"motion":0}],"textRight":"¡Deberías estar ansiosa por elso!"},{"chara":[{"cheek":1,"face":"mtn_ex_011.exp.json","id":100701,"voiceFull":"fullvoice/section_101855/vo_full_101801-10-41","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-40"}],"textRight":"*Risitas* ¡Seguro que sí!"},{"autoTurnLast":2.0,"turnChangeIn":"fadeIn","voiceFullStop":"fullvoice/section_101855/vo_full_101801-10-41"}]},"version":3}